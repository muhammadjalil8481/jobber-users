{
  "name": "5-users",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start ./dist/app.js -i 5 --attach | pino-pretty -C",
    "debug": "cross-env DEBUG=true tsx watch src/app.ts | pino-pretty -C\"",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "clean": "rimraf dist",
    "clear": "cross-env CLEAR_CMD=clear",
    "dev": "npm run clear && tsx watch src/app.ts | pino-pretty -C",
    "build": "npm run clean && tsc --project tsconfig.json && tsc-alias -p tsconfig.json && ts-node tools/copyEmails.ts",
    "test": "jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=false",
    "key": "tsx key-gen.ts",
    "db:create-migration": "npx sequelize-cli migration:generate --name",
    "db:create-seeder": "npx sequelize-cli seed:generate --name",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/amqplib": "^0.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.17",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "pino-pretty": "^13.0.0",
    "rimraf": "^6.0.1",
    "sequelize-cli": "^6.6.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-transform-paths": "^3.5.3"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.17.1",
    "@muhammadjalil8481/jobber-shared": "^0.7.32",
    "amqplib": "^0.10.8",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.7.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.16.3",
    "winston": "^3.17.0"
  }
}
